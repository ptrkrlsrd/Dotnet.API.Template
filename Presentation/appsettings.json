{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Tracing": {
        "Enabled": true
    },
    "Mediatr": {
        "Middleware": {
            "Tracing": true,
            "Logging": true
        }
    },
    "AllowedHosts": "*"
}