{
    "ApplicationName": "Dotnet.Template.API",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Information",
            "Microsoft.Hosting.Lifetime": "Warning"
        }
    },
    "Server": {
        "UseHTTPS": false,
        "Compression": {
            "EnableBrotli": true,
            "EnableGzip": true
        }
    },
    "Tracing": {
        "Enabled": false
    },
    "Mediatr": {
        "Middleware": {
            "Tracing": true,
            "Logging": true
        }
    },
    "AllowedHosts": "*"
}
